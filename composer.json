{
    "name": "vbergerondev/flux-tables",
    "description": "This is my package flux-tables",
    "keywords": [
        "Vincent Bergeron",
        "laravel",
        "flux-tables"
    ],
    "homepage": "https://github.com/vbergerondev/flux-tables",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Bergeron",
            "email": "hello@vbergeron.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "*",
        "livewire/flux": "^2.0",
        "livewire/flux-pro": "^2.0",
        "livewire/livewire": "^3.6",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0"
    },
    "autoload": {
        "psr-4": {
            "vbergerondev\\FluxTables\\": "src/",
            "vbergerondev\\FluxTables\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vbergerondev\\FluxTables\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "vbergerondev\\FluxTables\\FluxTablesServiceProvider"
            ],
            "aliases": {
                "FluxTables": "vbergerondev\\FluxTables\\Facades\\FluxTables"
            }
        }
    },
    "repositories": {
        "flux-pro": {
            "type": "composer",
            "url": "https://composer.fluxui.dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
